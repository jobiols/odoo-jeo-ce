<odoo>

<record id="report_percep_withhold_view" model="ir.ui.view">
    <field name="name">PERCEPTIONS WITHHOLDINGS REPORT</field>
    <field name="model">percep.withhold.wizard.report</field>
    <field name="arch" type="xml">
        <form>
            <group>
                <field name="date_from"/>
                <field name="date_to"/>
            </group>
            <footer>
                <button string="Generar Reporte" name="do_report"
                        type="object" class="oe_highlight"/>
                <button string="Cancel"
                        class="btn btn-secondary" special="cancel"/>
            </footer>
        </form>
    </field>
</record>

</odoo>
